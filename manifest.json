{
  "version": "1.0",
  "manifest_version": 3,
  "name": "Hey Googler",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "heyGoogler_icon.png"
  },
  "permissions": ["tabs", "activeTab", "history", "webNavigation", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["list.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
